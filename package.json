{
  "name": "yoyogift",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@sendgrid/mail": "^6.4.0",
    "axios": "^0.18.0",
    "core-js": "^3.1.2",
    "emailjs-com": "^2.3.2",
    "formik": "^1.5.7",
    "fs-extra": "^8.0.1",
    "history": "^4.9.0",
    "loggly-jslogger": "^2.2.2",
    "react": "^16.12.0",
    "react-app-rewire-workbox": "^2.0.1",
    "react-dom": "^16.12.0",
    "react-draggable": "^3.3.0",
    "react-google-login": "^5.1.10",
    "react-localize-redux": "^3.5.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.0.1",
    "react-star-rating-component": "^1.4.1",
    "react-test-renderer": "^16.4.2",
    "react-virtualized": "^9.21.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "throttle-debounce": "^2.1.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/registerServiceWorker.js",
      "!<rootDir>/src/custom-sw.js",
	  "!<rootDir>/src/store/index.js",
	  "!<rootDir>/src/shared/consts/array-consts.js",
	  "!<rootDir>/src/modules/common/utility/Random_rgba.js"
    ],
    "coverageReporters": [
      "text",
      "html"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    }
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "jest",
    "test:watch": "jest",
    "eject": "react-scripts eject",
    "test-coverage": "react-app-rewired test --env=jsdom --coverage",
    "test:ci": "react-app-rewired test --transformIgnorePatterns \"node_modules/(?!your-module-name)/\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.9.5",
    "@babel/preset-env": "^7.9.6",
    "axios-mock-adapter": "^1.18.1",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.4.4",
    "identity-obj-proxy": "^3.0.0",
    "react-app-rewired": "^2.1.5",
    "redux-mock-store": "^1.5.4"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  }
}
